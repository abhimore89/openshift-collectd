Hostname    "<%=ENV['OPENSHIFT_GEAR_DNS']%>"
#FQDNLookup   true
#BaseDir     "/var/lib/collectd"
#PluginDir   "${exec_prefix}/lib/collectd"
#TypesDB     "/usr/share/collectd/types.db"
#AutoLoadPlugin false

#Interval     10

#Timeout      2
#ReadThreads  5
#WriteThreads 5

LoadPlugin logfile

<Plugin logfile>
  LogLevel info
  File <%=ENV['OPENSHIFT_COLLECTD_DIR']%>/log/collectd.log
  Timestamp true
  PrintSeverity false
</Plugin>

